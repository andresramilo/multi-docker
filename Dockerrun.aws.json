{
    "AWSEBDDockerrunVersion": 2,
    "containerDefinitions": [
      {
         "name": "client",
         "image": "andresramilo/multi-client",
         "hostname": "client",
         "essential": false 
      },
      {
        "name": "server",
        "image": "andresramilo/multi-server",
        "hostname": "api",
        "essential": false 
     },
     {
        "name": "worker",
        "image": "andresramilo/multi-worker",
        "hostname": "worker",
        "essential": false 
     },
     {
        "name": "nginx",
        "image": "andresramilo/multi-worker",
        "essential": true,
        "portMappings": [
          {
            "hostPort":80,
            "containerPort":80
          }
        ],
        "links": ["client", "server"]
     }
    ]
}